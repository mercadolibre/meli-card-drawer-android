<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CardDrawerView">
        <attr name="card_header_internal_padding" format="dimension"/>
        <attr name="card_header_behaviour" format="enum">
            <enum name="regular" value="0"/>
            <enum name="responsive" value="1"/>
        </attr>
        <attr name="card_header_style" format="enum">
            <enum name="regular" value="0"/>
            <enum name="account_money_hybrid" value="1"/>
            <enum name="account_money_default" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CornerView">
        <attr name="corner_radius" format="dimension"/>
        <attr name="top_left_corner_radius" format="dimension"/>
        <attr name="top_right_corner_radius" format="dimension"/>
        <attr name="bottom_left_corner_radius" format="dimension"/>
        <attr name="bottom_right_corner_radius" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="BottomLabel">
        <attr name="style" format="enum">
            <enum name="black" value="0" />
            <enum name="bold" value="1" />
            <enum name="extra_bold" value="2" />
            <enum name="light" value="3" />
            <enum name="regular" value="4" />
            <enum name="semi_bold" value="5" />
            <enum name="medium" value="6" />
            <enum name="thin" value="7" />
            <enum name="monospace" value="8" />
        </attr>
    </declare-styleable>
</resources>